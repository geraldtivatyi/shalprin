{
    "name":"shalprin",
    "testing": false,
    "directories": {
        "global": {
            "from": ".",
            "dest": ".",
            "files": {
                "docker-compose.yml.tmpl": {}
            }
        },
        "gateway": {
            "dest": "/src/gateway",
            "directories": {
                "cmd": {
                    "directories": {
                        "test": {
                            "settings": "comment-filter",
                            "files": {
                                "main.go": {}
                            }
                        }
                    },
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                },
                "session": {
                    "settings": "comment-filter",
                    "files": {
                        "device.go": {},
                        "controller.go": {},
                        "model.go": {},
                        "view.go": {},
                        "record.go": {}
                    }
                }
            }
        },
        "profile": {
            "dest": "/src/profile",
            "settings": "comment-filter",
            "files": {
                "controller.go": {},
                "device.go": {},
                "model.go": {},
                "record.go": {},
                "view.go": {}
            },
            "directories": {
                "cmd": {
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                }
            }
        },
        "static": {
            "work-location": ".",
            "dest-location": "static"
        }
    },
    "execs": {
        "compile-gatewat": {
            "cmd": ["go", "build", "-o", "bin/gateway", "github.com/geraldtivatyi/shalprin/src/gateway/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        },
        "compile-profile": {
            "cmd": ["go", "build", "-o", "bin/profile", "github.com/geraldtivatyi/shalprin/src/profile/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        }
    }
}