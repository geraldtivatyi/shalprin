{
    "name":"shalprin",
    "work-location": ".",
    "dest-location": ".",
    "controls": {
        "behaviour": {
            "filters":{"comment-filter":{}}
        },
        "mappings": [
            {"start": "file:*go", "end": "exec:"}
        ]
    },
    "directories": {
        "shalprin": {
            "controls":{"behaviour": {"options":"output"}},
            "dst-ovr": "/work/shalprin",
            "directories": {
                "cmd": {
                    "controls":{"behaviour": {"filters":{"comment":{}}}},
                    "directories": {
                        "test": {
                            "controls":{"behaviour": {"filters":{"comment":{}}}},
                            "files": {
                                "main.go": {}
                            }
                        }
                    },
                    "files": {
                        "main.go": {}
                    }
                },
                "session": {
                    "files": {
                        "device.go": {},
                        "controller.go": {},
                        "model.go": {},
                        "view.go": {},
                        "record.go": {}
                    }
                },
                "profile": {
                    "files": {
                        "controller.go": {},
                        "device.go": {},
                        "model.go": {},
                        "record.go": {},
                        "view.go": {}
                    }
                }
            }
        },
        "static": {
            "dst-ovr": "/work/static",
            "controls": {
                "behaviour":{"options":"output"}
            },
            "directories": {
                "assets": {
                    "directories": {
                        "css": {
                            "directories": {
                                "images": {
                                    "settings": "copy-only",
                                    "files": {
                                        "bg01.png": {}
                                    }
                                }
                            },
                            "files": {
                                "font-awesome.min.css": {},
                                "ie8.css": {},
                                "lib.css": {},
                                "main.css": {}
                            }
                        },
                        "fonts": {
                            "files": {
                                "fontawesome-webfont.svg": {}
                            }
                        },
                        "js": {
                            "directories": {
                                "ie": {
                                    "files": {
                                        "html5shiv.js": {},
                                        "PIE.htc": {},
                                        "respond.min.js": {}
                                    }
                                }
                            },
                            "files": {
                                "jquery.dropotron.min.js": {},
                                "jquery.min.js": {},
                                "main.js": {},
                                "skel.min.js": {},
                                "util.js": {}
                            }
                        },
                        "sass": {
                            "directories": {
                                "libs": {
                                    "files": {
                                        "_functions.scss": {},
                                        "_mixins.scss": {},
                                        "_skel.scss": {},
                                        "_vars.scss": {}
                                    }
                                }
                            },
                            "settings": "copy-only",
                            "files": {
                                "ie8.scss": {},
                                "main.scss": {}
                            }
                        }
                    }
                },
                "images": {
                    "files": {
                        "bus.png": {},
                        "LocationA.png": {},
                        "lock.png": {},
                        "minusbutton.png": {},
                        "pic01.jpg": {},
                        "pic02.jpg": {},
                        "pic03.jpg": {},
                        "pic04.jpg": {},
                        "pic05.jpg": {},
                        "pic06.jpg": {},
                        "pic07.jpg": {},
                        "plusbutton.png": {},
                        "ProjectButton.png": {},
                        "SearchButton.png": {},
                        "SettingsButton.png": {},
                        "SignUpButton.png": {},
                        "star.png": {}
                    }
                }
            },
            "files": {
                "index.html": {},
                "left-sidebar.html": {},
                "no-sidebar.html": {},
                "signin.html": {},
                "signin_signup.html": {},
                "signup.html": {},
                "right-sidebar.html": {}
            }
        }
    },
    "execs": {
        "compile-shalprin": {
            "cmd": ["go", "build", "-o", "bin/shalprin", "github.com/geraldtivatyi/shalprin/work/shalprin/cmd/test"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        }
    }
}