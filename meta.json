{
    "name":"shalprin",
    "testing": false,
    "directories": {
        "global": {
            "from": ".",
            "dest": ".",
            "files": {
                "docker-compose.yml.tmpl": {}
            }
        },
        "gateway": {
            "dest": "/src/gateway",
            "directories": {
                "cmd": {
                    "directories": {
                        "test": {
                            "settings": "comment-filter",
                            "files": {
                                "main.go": {}
                            }
                        }
                    },
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                },
                "session": {
                    "settings": "comment-filter",
                    "files": {
                        "device.go": {},
                        "controller.go": {},
                        "model.go": {},
                        "view.go": {},
                        "record.go": {}
                    }
                }
            },
            "settings": "comment-filter",
            "files": {
                "index.go": {}
            }
        },
        "profile": {
            "dest": "/src/profile",
            "settings": "comment-filter",
            "files": {
                "controller.go": {},
                "device.go": {},
                "model.go": {},
                "record.go": {},
                "view.go": {}
            },
            "directories": {
                "cmd": {
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                }
            }
        },
        "static": {
            "directories": {
                "assets": {
                    "directories": {
                        "css": {
                            "directories": {
                                "images": {
                                    "settings": "copy-only",
                                    "files": {
                                        "bg01.png": {}
                                    }
                                }
                            },
                            "settings": "comment-filter",
                            "files": {
                                "font-awesome.min.css": {},
                                "ie8.css": {},
                                "lib.css": {},
                                "main.css": {}
                            }
                        },
                        "fonts": {
                            "settings": "copy-only",
                            "files": {
                                "fontawesome-webfont.eot": {},
                                "fontawesome-webfont.svg": {},
                                "fontawesome-webfont.ttf": {},
                                "fontawesome-webfont.woff": {},
                                "fontawesome-webfont.woff2": {},
                                "FontAwesome.otf": {}
                            }
                        },
                        "js": {
                            "directories": {
                                "ie": {
                                    "settings": "copy-only",
                                    "files": {
                                        "html5shiv.js": {},
                                        "PIE.htc": {},
                                        "respond.min.js": {}
                                    }
                                }
                            },
                            "settings": "copy-only",
                            "files": {
                                "jquery.dropotron.min.js": {},
                                "jquery.min.js": {},
                                "main.js": {},
                                "skel.min.js": {},
                                "util.js": {}
                            }
                        },
                        "sass": {
                            "directories": {
                                "libs": {
                                    "settings": "copy-only",
                                    "files": {
                                        "_functions.scss": {},
                                        "_mixins.scss": {},
                                        "_skei.scss": {},
                                        "_vars.scss": {}
                                    }
                                }
                            },
                            "settings": "copy-only",
                            "files": {
                                "ie8.scss": {},
                                "main.scss": {}
                            }
                        }
                    }
                },
                "images": {
                    "files": {
                        "AccountsButton.png": {},
                        "bus.png": {},
                        "LocationA.png": {},
                        "lock.png": {},
                        "minusbutton.png": {},
                        "pic01.png": {},
                        "pic02.png": {},
                        "pic03.png": {},
                        "pic04.png": {},
                        "pic05.png": {},
                        "pic06.png": {},
                        "pic07.png": {},
                        "plusbutton.png": {},
                        "ProjectButton.png": {},
                        "SearchButton.png": {},
                        "SettingsButton.png": {},
                        "SignUpButton.png": {},
                        "star.png": {}
                    }
                }
            },
            "settings": "copy-only",
            "files": {
                "index.html": {},
                "left-sidebar.html": {},
                "no-sidebar.html": {},
                "right-sidebar.html": {}
            }
        }
    },
    "execs": {
        "compile-gateway": {
            "cmd": ["go", "build", "-o", "bin/gateway", "github.com/geraldtivatyi/shalprin/src/gateway/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        },
        "compile-profile": {
            "cmd": ["go", "build", "-o", "bin/profile", "github.com/geraldtivatyi/shalprin/src/profile/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        }
    }
}