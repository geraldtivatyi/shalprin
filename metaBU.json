{
    "name":"shalprin",
    "testing": false,
    "directories": {
        "global": {
            "from": ".",
            "dest": ".",
            "files": {
                "docker-compose.yml.tmpl": {}
            }
        },
        "gateway": {
            "dest": "/src/gateway",
            "directories": {
                "cmd": {
                    "directories": {
                        "test": {
                            "settings": "comment-filter",
                            "files": {
                                "main.go": {}
                            }
                        }
                    },
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                },
                "session": {
                    "settings": "comment-filter",
                    "files": {
                        "device.go": {},
                        "controller.go": {},
                        "model.go": {},
                        "view.go": {},
                        "record.go": {}
                    }
                }
            }
        },
        "profile": {
            "dest": "/src/profile",
            "settings": "comment-filter",
            "files": {
                "controller.go": {},
                "device.go": {},
                "model.go": {},
                "record.go": {},
                "view.go": {}
            },
            "directories": {
                "cmd": {
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                }
            }
        },
        "static": {
            "work-location": ".",
            "dest-location": "static",
            "directories": {
                "assets": {
                    "directories": {
                        "css": {
                            "directories": {
                                "images": {
                                    "settings": "comment-filter",
                                    "files": {
                                        "bg01.png": {
                                            "templates": ["static/assets/css/images/bg01.png"]
                                        }
                                    }
                                }
                            },
                            "settings": "comment-filter",
                            "files": {
                                "font-awesome.min.css": {
                                    "templates": ["static/assets/css/font-awesome.min.css"]
                                },
                                "ie8.css": {
                                    "templates": ["static/assets/css/ie8.css"]
                                },
                                "lib.css": {
                                    "templates": ["static/assets/css/lib.css"]
                                },
                                "main.css": {
                                    "templates": ["static/assets/css/main.css"]
                                }
                            }
                        },
                        "fonts": {
                            "settings": "comment-filter",
                            "files": {
                                "fontawesome-webfont.eot": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.eot"]
                                },
                                "fontawesome-webfont.svg": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.svg"]
                                },
                                "fontawesome-webfont.ttf": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.ttf"]
                                },
                                "fontawesome-webfont.woff": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.woff"]
                                },
                                "fontawesome-webfont.woff2": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.woff2"]
                                },
                                "FontAwesome.otf": {
                                    "templates": ["static/assets/fonts/FontAwesome.otf"]
                                }
                            }
                        },
                        "js": {
                            "directories": {
                                "ie": {
                                    "settings": "comment-filter",
                                    "files": {
                                        "html5shiv.js": {
                                            "templates": ["static/assets/js/ie/html"]
                                        },
                                        "PIE.htc": {},
                                        "respond.min.ks": {}
                                    }
                                }
                            },
                            "settings": "comment-filter",
                            "files": {
                                "jquery.dropotron.min.js": {},
                                "jquery.min.js": {},
                                "main.js": {},
                                "skel.min.js": {},
                                "util.js": {}
                            }
                        },
                        "sass": {
                            "directories": {
                                "libs": {
                                    "settings": "comment-filter",
                                    "files": {
                                        "_functions.scss": {},
                                        "_mixins.scss": {},
                                        "_skei.scss": {},
                                        "_vars.scss": {}
                                    }
                                }
                            },
                            "settings": "comment-filter",
                            "files": {
                                "ie8.scss": {},
                                "main.scss": {}
                            }
                        }
                    }
                },
                "images": {
                    "files": {
                        "AccountsButton.png": {},
                        "bus.png": {},
                        "LocationA.png": {},
                        "lock.png": {},
                        "minusbutton.png": {},
                        "pic01.png": {},
                        "pic02.png": {},
                        "pic03.png": {},
                        "pic04.png": {},
                        "pic05.png": {},
                        "pic06.png": {},
                        "pic07.png": {},
                        "plusbutton.png": {},
                        "ProjectButton.png": {},
                        "SearchButton.png": {},
                        "SettingsButton.png": {},
                        "SignUpButton.png": {},
                        "star.png": {}
                    }
                },
                "files": {
                    "index.html": {},
                    "left-sidebar.html": {},
                    "no-sidebar.html": {},
                    "right-sidebar.html": {}
                }
            }
        }
    },
    "execs": {
        "compile-gatewat": {
            "cmd": ["go", "build", "-o", "bin/gateway", "github.com/geraldtivatyi/shalprin/src/gateway/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        },
        "compile-profile": {
            "cmd": ["go", "build", "-o", "bin/profile", "github.com/geraldtivatyi/shalprin/src/profile/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        }
    }
}