{
    "name":"shalprin",
    "testing": false,
    "directories": {
        "global": {
            "from": ".",
            "dest": ".",
            "files": {
                "docker-compose.yml.tmpl": {}
            }
        },
        "gateway": {
            "dest": "/src/gateway",
            "directories": {
                "cmd": {
                    "directories": {
                        "test": {
                            "settings": "comment-filter",
                            "files": {
                                "main.go": {}
                            }
                        }
                    },
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                },
                "session": {
                    "settings": "comment-filter",
                    "files": {
                        "device.go": {},
                        "controller.go": {},
                        "model.go": {},
                        "view.go": {},
                        "record.go": {}
                    }
                }
            }
        },
        "profile": {
            "dest": "/src/profile",
            "settings": "comment-filter",
            "files": {
                "controller.go": {},
                "device.go": {},
                "model.go": {},
                "record.go": {},
                "view.go": {}
            },
            "directories": {
                "cmd": {
                    "settings": "comment-filter",
                    "files": {
                        "main.go": {}
                    }
                }
            }
        },
        "static": {
            "work-location": ".",
            "dest-location": "static",
            "directories": {
                "assets": {
                    "directories": {
                        "css": {
                            "directories": {
                                "images": {
                                    "settings": "comment-filter",
                                    "files": {
                                        "bg01.png": {
                                            "templates": ["static/assets/css/images/bg01.png"]
                                        }
                                    }
                                }
                            },
                            "settings": "comment-filter",
                            "files": {
                                "font-awesome.min.css": {
                                    "templates": ["static/assets/css/font-awesome.min.css"]
                                },
                                "ie8.css": {
                                    "templates": ["static/assets/css/ie8.css"]
                                },
                                "lib.css": {
                                    "templates": ["static/assets/css/lib.css"]
                                },
                                "main.css": {
                                    "templates": ["static/assets/css/main.css"]
                                }
                            }
                        },
                        "fonts": {
                            "settings": "comment-filter",
                            "files": {
                                "fontawesome-webfont.eot": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.eot"]
                                },
                                "fontawesome-webfont.svg": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.svg"]
                                },
                                "fontawesome-webfont.ttf": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.ttf"]
                                },
                                "fontawesome-webfont.woff": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.woff"]
                                },
                                "fontawesome-webfont.woff2": {
                                    "templates": ["static/assets/fonts/fontawesome-webfont.woff2"]
                                },
                                "FontAwesome.otf": {
                                    "templates": ["static/assets/fonts/FontAwesome.otf"]
                                }
                            }
                        },
                        "js": {
                            "directories": {
                                "ie": {
                                    "settings": "comment-filter",
                                    "files": {
                                        "html5shiv.js": {
                                            "templates": ["static/assets/js/ie/html5shiv.js"]
                                        },
                                        "PIE.htc": {
                                            "templates": ["static/assets/js/ie/PIE.htc"]
                                        },
                                        "respond.min.js": {
                                            "templates": ["static/assets/js/ie/respond.min.js"]
                                        }
                                    }
                                }
                            },
                            "settings": "comment-filter",
                            "files": {
                                "jquery.dropotron.min.js": {
                                    "templates": ["static/assets/js/jquery.dropotron.min.js"]
                                },
                                "jquery.min.js": {
                                    "templates": ["static/assets/js/jquery.min.js"]
                                },
                                "main.js": {
                                    "templates": ["static/assets/js/main.js"]
                                },
                                "skel.min.js": {
                                    "templates": ["static/assets/js/skel.min.js"]
                                },
                                "util.js": {
                                    "templates": ["static/assets/js/util.js"]
                                }
                            }
                        },
                        "sass": {
                            "directories": {
                                "libs": {
                                    "settings": "comment-filter",
                                    "files": {
                                        "_functions.scss": {
                                            "templates": ["static/assets/sass/libs/_functions.scss"]
                                        },
                                        "_mixins.scss": {
                                            "templates": ["static/assets/sass/libs/_mixins.scss"]
                                        },
                                        "_skei.scss": {
                                            "templates": ["static/assets/sass/libs/_skei.scss"]
                                        },
                                        "_vars.scss": {
                                            "templates": ["static/assets/sass/libs/_vars.scss"]
                                        }
                                    }
                                }
                            },
                            "settings": "comment-filter",
                            "files": {
                                "ie8.scss": {
                                    "templates": ["static/assets/sass/ie8.scss"]
                                },
                                "main.scss": {
                                    "templates": ["static/assets/sass/main.scss"]
                                }
                            }
                        }
                    }
                },
                "images": {
                    "files": {
                        "AccountsButton.png": {
                            "templates": ["static/images/AccountsButton.png"]
                        },
                        "bus.png": {
                            "templates": ["static/images/bus.png"]
                        },
                        "LocationA.png": {
                            "templates": ["static/images/LocationA.png"]
                        },
                        "lock.png": {
                            "templates": ["static/images/lock.png"]
                        },
                        "minusbutton.png": {
                            "templates": ["static/images/minusbutton.png"]
                        },
                        "pic01.png": {
                            "templates": ["static/images/pic01.png"]
                        },
                        "pic02.png": {
                            "templates": ["static/images/pic02.png"]
                        },
                        "pic03.png": {
                            "templates": ["static/images/pic03.png"]
                        },
                        "pic04.png": {
                            "templates": ["static/images/pic04.png"]
                        },
                        "pic05.png": {
                            "templates": ["static/images/pic05.png"]
                        },
                        "pic06.png": {
                            "templates": ["static/images/pic06.png"]
                        },
                        "pic07.png": {
                            "templates": ["static/images/pic07.png"]
                        },
                        "plusbutton.png": {
                            "templates": ["static/images/plusbutton.png"]
                        },
                        "ProjectButton.png": {
                            "templates": ["static/images/ProjectButton.png"]
                        },
                        "SearchButton.png": {
                            "templates": ["static/images/SearchButton.png"]
                        },
                        "SettingsButton.png": {
                            "templates": ["static/images/SettingsButton.png"]
                        },
                        "SignUpButton.png": {
                            "templates": ["static/images/SignUpButton.png"]
                        },
                        "star.png": {
                            "templates": ["static/images/star.png"]
                        }
                    }
                },
                "files": {
                    "index.html": {
                        "templates": ["static/index.html"]
                    },
                    "left-sidebar.html": {
                        "templates": ["static/left-sidebar.html"]
                    },
                    "no-sidebar.html": {
                        "templates": ["static/no-sidebar.html"]
                    },
                    "right-sidebar.html": {
                        "templates": ["static/right-sidebar.html"]
                    }
                }
            }
        }
    },
    "execs": {
        "compile-gatewat": {
            "cmd": ["go", "build", "-o", "bin/gateway", "github.com/geraldtivatyi/shalprin/src/gateway/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        },
        "compile-profile": {
            "cmd": ["go", "build", "-o", "bin/profile", "github.com/geraldtivatyi/shalprin/src/profile/cmd"],
            "timeout": 100000,
            "env": {"GOOS":"linux"}
        }
    }
}